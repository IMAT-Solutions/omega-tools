{
  "all": true,
  "cache": false,
  "check-coverage": true,
  "eager": true,
  "exclude": [
    "lib/**/*.mocha.js"
  ],
  "extension": [],
  "include": [
    "lib/**/*.js"
  ],
  "instrument": true,
  "per-file": false,
  "report-dir": "coverage/mocha",
  "reporter": [
    "lcov",
    "text-summary"
  ],
  "require": [],
  "branches": 40,
  "functions": 50,
  "lines": 50,
  "statements": 50,
  "watermarks": {
    "lines": [60, 90],
    "functions": [60, 90],
    "branches": [60, 90],
    "statements": [60, 90]
  }
}
